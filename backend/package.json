{
  "name": "innergy-backend",
  "version": "1.0.0",
  "description": "Backend Lambda functions for Innergy Blueprint Detection",
  "scripts": {
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "logs:upload": "serverless logs -f uploadHandler -t",
    "logs:inference": "serverless logs -f inferenceHandler -t",
    "logs:results": "serverless logs -f resultsHandler -t"
  },
  "devDependencies": {
    "serverless": "^3.38.0",
    "serverless-python-requirements": "^6.1.0"
  }
}
